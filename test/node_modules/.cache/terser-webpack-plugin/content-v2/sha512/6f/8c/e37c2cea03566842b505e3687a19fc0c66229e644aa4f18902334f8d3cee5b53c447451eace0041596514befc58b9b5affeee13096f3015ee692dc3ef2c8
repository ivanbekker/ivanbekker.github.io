d(item => item.value === active);\\n      const paySum =\\n        status === 'year' ? activeItem.price * 0.9 : activeItem.price;\\n      this.pay({ pay: paySum * 100, value: active, status });\\n    }\\n  }\\n\\n  // получить выбранный тариф и период из хеша\\n  getHashdata() {\\n    // if (hashData.length > 0) {\\n    //   const hash = hashData.slice(1).split('-');\\n    //   return {\\n    //     active: hash[0],\\n    //     status: hash[1],\\n    //     pay: !!hash[2],\\n    //   };\\n    // }\\n    return {};\\n  }\\n\\n  // усановить выбранный тариф и период в хеш\\n  setPricingHashFilter(active, status, pay) {\\n    if (typeof window !== 'undefined') {\\n      if (pay) {\\n        window.location.hash = `${active}-${status}-pay`;\\n      } else {\\n        window.location.hash = `${active}-${status}`;\\n      }\\n    }\\n  }\\n\\n  // задать период подписки\\n  changeStatus(status) {\\n    if (status === this.state.status) return;\\n    this.setState({\\n      status,\\n    });\\n  }\\n\\n  loading(loading) {\\n    this.setState({\\n      loading,\\n    });\\n  }\\n\\n  getPeriodValue(value, status) {\\n    if (value === 'XS') {\\n      return status === 'year' ? 'yearly' : 'monthly';\\n    }\\n    return status === 'year' ? 'y' : 'm';\\n  }\\n\\n  pay(data) {\\n    const host = process.env.GATSBY_HOST;\\n    this.loading(true);\\n\\n    UtilsApi.isAuth(res => {\\n      if (!res.login) {\\n        // const location = encodeURIComponent(`${document.location.href}-pay`);\\n        document.location.href = `${host}/auth2/single_account?redirect_uri=${encodeURIComponent(document.location)}`;\\n      } else {\\n        const period = this.getPeriodValue(data.value, data.status);\\n        const tariff = `${data.value}_${period}`;\\n        UtilsApi.initializePay(tariff, resPay => {\\n          this.openStripeCheckout({\\n            ...data,\\n            login: res.login,\\n            orderId: resPay.ops[0].order_id,\\n          });\\n        });\\n      }\\n    });\\n  }\\n\\n  openStripeCheckout({ pay, value, status, login, orderId }) {\\n    this.stripeHandler.open({\\n      name: value.toUpperCase(),\\n      amount: status === 'year' ? pay * 12 : pay,\\n      description: status === 'year' ? 'Yearly plan' : 'Monthly plan',\\n      email: login,\\n      opened: () => this.loading(false),\\n      token: token => {\\n        // callback на завершенный чекаут stripe\\n        UtilsApi.createPay(orderId, token.id, this.redirect);\\n      },\\n    });\\n  }\\n\\n  // переход на страницу биллинга после оплаты stripe\\n  redirect() {\\n    if (typeof window !== 'undefined') {\\n      const host = process.env.GATSBY_HOST;\\n      window.location.replace(`${host}/billing`);\\n    }\\n  }\\n\\n  // обработчик клика на тарифный план\\n  handleClick(item) {\\n    if (item.value === this.state.active) return;\\n    this.setState({\\n      active: item.value,\\n    });\\n  }\\n\\n  // рендер списка тарифных планов\\n  renderPrising(active) {\\n    return this.tariffs.map((item, index) => (\\n      <PriceItem\\n        key={Utils.random()}\\n        item={item}\\n        active={item.value === active}\\n        firstItem={index === 0}\\n        lastItem={index === this.tariffs.length - 1}\\n        handleClick={() => this.handleClick(item)}\\n      />\\n    ));\\n  }\\n\\n  changeViewContactSales() {\\n    this.setState({\\n      viewContactSales: !this.state.viewContactSales,\\n    });\\n  }\\n\\n  render() {\\n    const { intl } = this.props;\\n    const { status, active, loading } = this.state;\\n    this.tariffs = pricing.tariffs;\\n    const activeItem = this.tariffs.find(item => item.value === active);\\n\\n    let paySum = '';\\n    let labelBtn = '';\\n    if (activeItem) {\\n      paySum = status === 'year' ? activeItem.price * 0.9 : activeItem.price;\\n      labelBtn = `$${paySum.toLocaleString()}`;\\n    }\\n\\n    const periodMonthClass =\\n      status === 'month'\\n        ? 'pricing__corezoid__bottom__buttons__btn acti