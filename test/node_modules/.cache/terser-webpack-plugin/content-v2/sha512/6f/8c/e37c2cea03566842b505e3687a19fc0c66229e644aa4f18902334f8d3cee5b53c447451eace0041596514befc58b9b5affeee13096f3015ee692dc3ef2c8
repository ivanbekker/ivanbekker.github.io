__pay__left__item', {active: payMethod === 'invoice'})}\\n                    onClick={() => this.handleChangePayMethod('invoice')}\\n                  >\\n                    <Label value={intl.formatMessage({id: pricing.pay.invoice})}/>\\n                  </div>\\n                </div>\\n                {payMethod === 'invoice' &&\\n                  <Form\\n                    data={invoice}\\n                    options={{\\n                      type,\\n                      license,\\n                      price: labelBtn\\n                    }}\\n                    forceSend={sendForm}\\n                    callback={() => this.sendForm()}\\n                  />\\n                }\\n              </div>\\n              <div className=\\\"pricing__pay__right\\\">\\n                <Label value={intl.formatMessage({id: pricing.pay.summary})}/>\\n                <div className=\\\"pricing__pay__right__rows\\\">\\n                  {rows.map((row, index) => (\\n                    <div\\n                      key={index}\\n                      className={cn('pricing__pay__right__row', {\\n                        border: row.border,\\n                        total: row.total,\\n                        show: row.show\\n                      })}\\n                    >\\n                      <Label value={row.key} />\\n                      <Label value={row.value} />\\n                    </div>\\n                  ))}\\n                  <Button\\n                    className={cn(\\n                      'pricing__pay__bottom__btn button__default',\\n                    )}\\n                    label={intl.formatMessage({id: pricing.pay.buy})}\\n                    size={'small'}\\n                    onClick={() =>\\n                      this.pay({\\n                        pay: paySum * 100,\\n                        value: license,\\n                        status,\\n                      }, payMethod)\\n                    }\\n                  />\\n 