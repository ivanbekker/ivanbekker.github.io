 { type = 'corezoid', tariff = 'XS', pay = false } = this.getHashdata(\\n      this.props.hashData,\\n    );\\n    this.setState({\\n      type,\\n      tariff,\\n      pay,\\n    });\\n    this.setPricingHashFilter(type, tariff, pay);\\n  }\\n\\n  componentDidMount() {\\n    let key = '';\\n    if (\\n      process.env.GATSBY_HOST.indexOf('dev') !== -1 ||\\n      process.env.GATSBY_HOST.indexOf('pre') !== -1\\n    ) {\\n      key = 'pk_test_xvUkvKX7lQSty7oV9OoSCDQZ';\\n    } else {\\n      key = 'pk_live_rHVS21A6ssJRlwXdiptwGhwY';\\n    }\\n\\n    this.stripeHandler = StripeCheckout.configure({\\n      key,\\n      closed: () => {\\n        // callback на закрытие окна stripe\\n        this.setState({\\n          pay: false,\\n        });\\n      },\\n    });\\n    if (window) {\\n      this.setFilter();\\n    }\\n  }\\n\\n  componentDidUpdate() {\\n    const { type, tariff, pay } = this.state;\\n    this.setPricingHashFilter(type, tariff, pay);\\n    // if (pay) {\\n    //   // запуск оплаты stripe, если в hash есть параметр -pay\\n    //   const activeItem = this.tariffs.find(item => item.value === active);\\n    //   const paySum =\\n    //     status === activeItem.price;\\n    //   this.pay({ pay: paySum * 100, value: fariff, status });\\n    // }\\n  }\\n\\n  // получить выбранный тариф и период из хеша\\n  getHashdata(hashData) {\\n    if (hashData.length > 0) {\\n      const hash = hashData.slice(1).split('-');\\n      return {\\n        type: hash[0],\\n        tariff: hash[1],\\n        pay: !!hash[2],\\n      };\\n    }\\n    return {};\\n  }\\n\\n  // усановить выбранный тариф и период в хеш\\n  setPricingHashFilter(type, tariff, pay) {\\n    if (typeof window !== 'undefined') {\\n      if (pay) {\\n        window.location.hash = `${type}-${tariff}-pay`;\\n      } else {\\n        window.location.hash = `${type}-${tariff}`;\\n      }\\n    }\\n  }\\n\\n  // задать период подписки\\n  changeStatus(status) {\\n    if (status === this.state.status) return;\\n    this.setState({\\n      status,\\n    });\\n  }\\n\\n  // loading(loading) {\\n  //   this.setState({\\n  //     loading,\\n  //   });\\n  // }\\n\\n  getPeriodValue(value) {\\n    if (value === 'XS') {\\n      return 'monthly';\\n    }\\n    return 'm';\\n  }\\n\\n  pay(data, type) {\\n    const host = process.env.GATSBY_HOST;\\n    // this.loading(true);\\n\\n    UtilsApi.isAuth(res => {\\n      if (!res.login) {\\n        // const location = encodeURIComponent(`${document.location.href}-pay`);\\n        document.location.href = `${host}/auth2/single_account?redirect_uri=${encodeURIComponent(document.location)}`;\\n      } else {\\n        if (type === 'invoice') {\\n          this.setState({\\n            sendForm: true\\n          });\\n          this.setState({\\n            sendForm: false\\n          });\\n          return;\\n        }\\n        const period = this.getPeriodValue(data.value);\\n        const tariff = `${data.value}_${period}`;\\n        UtilsApi.initializePay(tariff, resPay => {\\n          this.openStripeCheckout({\\n            ...data,\\n            login: res.login,\\n            orderId: resPay.ops[0].order_id,\\n          });\\n        });\\n      }\\n    });\\n  }\\n\\n  sendForm() {\\n    this.props.linkTo('sendInvoice');\\n  }\\n\\n  openStripeCheckout({ pay, value, login, orderId }) {\\n    this.stripeHandler.open({\\n      name: value.toUpperCase(),\\n      amount: pay,\\n      description: 'Monthly plan',\\n      email: login,\\n      // opened: () => this.loading(false),\\n      token: token => {\\n        // callback на завершенный чекаут stripe\\n        UtilsApi.createPay(orderId, token.id, this.redirect);\\n      },\\n    });\\n  }\\n\\n  // переход на страницу биллинга после оплаты stripe\\n  redirect() {\\n    if (typeof window !== 'undefined') {\\n      const host = process.env.GATSBY_HOST;\\n      window.location.replace(`${host}/billing`);\\n    }\\n  }\\n\\n  // обработчик клика на тарифный план\\n  handleClick(i