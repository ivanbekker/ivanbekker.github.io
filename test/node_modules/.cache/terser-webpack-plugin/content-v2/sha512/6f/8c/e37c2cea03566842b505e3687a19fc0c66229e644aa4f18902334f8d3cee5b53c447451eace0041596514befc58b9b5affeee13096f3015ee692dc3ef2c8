3',\\n          {\\n            label: !p.title,\\n            border: p.title && type !== 'title' && type !== 'price',\\n            title: type === 'title',\\n            titleBase: type === 'title' && p.title === 'supportListBaseTitle',\\n            titleBusiness: type === 'title' && p.title === 'supportListBusinessTitle',\\n            titleEnterprise: type === 'title' && p.title === 'supportListInterpriseTitle',\\n            price: type === 'price',\\n            priceSup: type === 'price' && p.title,\\n            priceBase: type === 'price' && p.title === 'supportListBaseTitle',\\n            priceBusiness: type === 'price' && p.title === 'supportListBusinessTitle',\\n            priceEnterprise: type === 'price' && p.title === 'supportListInterpriseTitle',\\n          }\\n        )}\\n      >\\n        {p[type] &&\\n          <span\\n            dangerouslySetInnerHTML={{\\n              __html: Utils.bbCodeToHtml(intl.formatMessage({id: p[type]}))\\n            }}\\n          />\\n        }\\n      </div>\\n    ));\\n  }\\n\\n  renderSupport(support) {\\n    return (\\n      <div className=\\\"pricing__support__pricing__scrollContainer\\\">\\n        <div className=\\\"grid-wrapper pricing__support__pricing\\\">\\n          {this.renderRow(support, 'title')}\\n          {this.renderRow(support, 'service')}\\n          {this.renderRow(support, 'health')}\\n          {this.renderRow(support, 'techSupport')}\\n          {this.renderRow(support, 'channels')}\\n          {this.renderRow(support, 'severity')}\\n          {this.renderRow(support, 'archSupport')}\\n          {this.renderRow(support, 'envSupport')}\\n          {this.renderRow(support, 'guidance')}\\n          {this.renderRow(support, 'lauSupport')}\\n          {this.renderRow(support, 'trainings')}\\n          {this.renderRow(support, 'price')}\\n        </div>\\n      </div>\\n    );\\n  }\\n\\n  render() {\\n    const { intl } = this.props;\\n\\n    return (\\n      <div className=\\\"pricing__s